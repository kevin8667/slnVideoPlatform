@model Backstage.Models.SeasonList

@{
    ViewData["Title"] = "Details";
}

<h1>季詳細</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SeriesId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SeriesId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SeasonName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SeasonName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SeasonNumber)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SeasonNumber)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EpisodeCount)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EpisodeCount)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ReleaseDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ReleaseDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Summary)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Summary)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.SeasonId" class="btn btn-primary">編輯</a> |
    <a asp-action="Index" class="btn btn-primary">回到列表</a>
</div>
